<Window x:Class="pr43savichev.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pr43savichev"
        mc:Ignorable="d" Height="450" Width="800" MinHeight="450" MinWidth="800" Title="Студенты и курсы в учебном центре">
    <Grid Background="White">
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Margin="5,0,5,5" Background="#FFFAFAFA" Height="374" VerticalAlignment="Bottom"/>
        <Button Command="{Binding OpenStudents}" Width="151" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0"></Button>
        <Button Command="{Binding OpenCourses}" Width="151" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="196,10,0,0"></Button>
    </Grid>
</Window>
